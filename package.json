  },
  "bugs": {
    "url": "https://github.com/Nathaniel Inman/space-invaders/issues"
  },
  "scripts": {
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "build:assets": "cp -rf src/assets/* ./dist",
    "build": "npm run build:clean && npm run build:assets",
    "prestart": "npm run build",
    "start": "node ./node_modules/webpack/bin/webpack.js --config ./config.babel.js --watch",
    "preproduction": "npm run build",
    "production": "node ./node_modules/webpack/bin/webpack.js --config ./config.babel.js",
    "deploy": "./deploy.sh"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-sync": "^2.18.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.9.0",
    "jade-static-loader": "^0.3.0",
    "poststylus": "^0.2.3",
    "source-loader": "^0.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.com/Nathaniel Inman/space-invaders",
  "dependencies": {
    "ion-cloud": "^0.3.0"
  }
}
